@model ZelectroCom.Web.ViewModels.Section.SectionVm
@{
    ViewBag.Title = Model.Name;
    ViewBag.PageDescription = Model.Description;
    ViewBag.PageTitle = Model.Name;
}

@section Meta {
    <meta name="title" content="@Model.SeoTitle"/>
    <meta name="description" content="@Model.SeoDescription"/>
    <meta name="keywords" content="@Model.SeoKeywords" />
}

@Html.Action("SectionPosts", "Post", new { id = Model.Id, path = Model.Path, page = Model.Page})
